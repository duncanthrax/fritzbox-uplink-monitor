<schemalist gettext-domain="system-monitor">
	<schema id="org.gnome.shell.extensions.fritzbox-uplink-monitor" path="/org/gnome/shell/extensions/fritzbox-uplink-monitor/">
		<key name="fritzbox-ip" type="s">
			<default>''</default>
			<summary>Fritzbox hostname/IP</summary>
			<description>Hostname or IP of your Fritzbox. When left empty, defaults to "fritz.box".</description>
		</key>

		<key name="chart-width" type="i">
			<default>60</default>
			<summary>Charts width</summary>
			<description>Width in pixels for the uplink and downlink charts.</description>
		</key>

		<key name="downstream-total-color" type="s">
			<default>'#fce94f'</default>
			<summary>Color for total downstream traffic</summary>
		</key>

		<key name="downstream-other-color" type="s">
			<default>'#fce94f'</default>
			<summary>Color for TV/Phone downstream traffic</summary>
		</key>

		<key name="upstream-total-color" type="s">
			<default>'#fce94f'</default>
			<summary>Color for total upstream traffic</summary>
		</key>

		<key name="upstream-other-color" type="s">
			<default>'#fce94f'</default>
			<summary>Color for TV/Phone upstream traffic</summary>
		</key>
	</schema>
</schemalist>
